{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "KafkaSettings": {
    "BootstrapServers": "kafka:9092",
    "ConsumerGroupId": "notifications-group",
    "BatchSize": 16384,
    "LingerMs": 5,
    "CompressionType": "Snappy"
  }
}
